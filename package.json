{
  "name": "bloggingapi",
  "version": "1.0.0",
  "description": "blogging service providing REST API for developpers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "buildDB-prod": "cross-env NODE_ENV=production node config/buildDB.js",
    "buildDB": "node config/buildDB.js",
    "pretest": "eslint --ignore-path .eslintignore --ignore-pattern *.test.js  .",
    "test": "jest",
    "coverage": "jest --collectCoverageFrom=src/**.js --collectCoverageFrom=app.js --coverage test",
    "deploy": "git add . && git commit -m\"deploy commit\" && git push origin master && git push heroku master && heroku open"
  },
  "author": "Daria",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.13.3",
    "cloudinary": "^1.11.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.2.0",
    "gulp-if": "^2.0.2",
    "jest": "^23.1.0",
    "knex": "^0.14.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "slugify": "^1.3.0",
    "winston": "^3.0.0-rc6"
  },
  "devDependancies": {
    "eslint": "^4.19.1",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "cross-env": "^5.1.6",
    "node-mocks-http": "^1.7.0",
    "regenerator-runtime": "^0.11.1",
    "supertest": "^3.1.0"
  }
